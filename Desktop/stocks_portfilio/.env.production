# Production Environment Variables for Vercel Deployment
# Copy these to your Vercel dashboard under Environment Variables

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Clerk Authentication (if using)
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here

# API Configuration
VITE_API_BASE_URL=https://your-domain.vercel.app

# Stock API Configuration (if using external APIs)
VITE_STOCK_API_KEY=your_stock_api_key_here
VITE_NEWS_API_KEY=your_news_api_key_here

# Application Settings
VITE_APP_NAME=Stock Portfolio Tracker
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production